{
    "name": "mrshanebarron/button",
    "description": "Customizable button component for Laravel - supports Livewire and Vue",
    "keywords": ["laravel", "livewire", "vue", "button", "ui"],
    "license": "MIT",
    "authors": [{"name": "Shane Barron", "email": "mrshanebarron@gmail.com"}],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MrShaneBarron\\Button\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["MrShaneBarron\\Button\\ButtonServiceProvider"]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
